{
  "name": "binary-next-gen",
  "main": "src/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-com/binary-next-gen.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require babel-polyfill ./src/**/__tests__/*.js -R progress",
    "test:full": "npm run test:eslint && npm run test:coveralls",
    "test:eslint": "webpack --config webpack.config.js",
    "test:unit": "mocha --compilers js:babel-core/register --require ignore-styles --require babel-polyfill ./src/**/__tests__/*.js",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- --require ignore-styles src/**/__tests__/*.js",
    "test:watch": "npm test -- --watch",
    "test:unit-watch": "mocha --compilers js:babel-core/register --require ignore-styles --require babel-polyfill --watch ./src/**/__tests__/*.js",
    "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "node server.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint src",
    "electron": "electron electron.js",
    "translate": "bash fetch-translation.sh"
  },
  "keywords": [
    "binary.com",
    "webpack",
    "react",
    "redux",
    "websockets"
  ],
  "author": "Boris, Qing Wei, Nuru @ Binary",
  "license": "MIT",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-function-bind",
      "transform-decorators-legacy"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-inline-elements",
          "transform-react-constant-elements"
        ]
      }
    }
  },
  "dependencies": {
    "binary-charts": "*",
    "binary-components": "^1.0.0",
    "binary-live-api": "*",
    "binary-utils": "^3.3.6",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-intl": "^2.1.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "4.4.5",
    "react-router": "^2.4.1",
    "react-sparklines": "*",
    "react-youtube": "^6.1.0",
    "redux": "^3.5.2",
    "redux-debounced": "^0.2.0",
    "redux-storage": "^4.0.0",
    "redux-storage-decorator-debounce": "^1.0.0",
    "redux-storage-decorator-filter": "^1.1.4",
    "redux-storage-merger-immutablejs": "^1.0.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "reselect-map": "0.0.1",
    "trackjs": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "chai-subset": "^1.2.2",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "~2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-file": "^0.3.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^2.3.1",
    "gulp-shell": "^0.5.2",
    "gulp-util": "^3.0.7",
    "ignore-styles": "^2.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "postcss-loader": "^0.9.1",
    "react-addons-perf": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-mock-store": "1.1.0",
    "run-sequence": "^1.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-notifier": "^1.3.0"
  }
}
