{
  "name": "binary-next-gen",
  "main": "src/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-com/binary-next-gen.git"
},
  "scripts": {
    "test": "npm run test:unit",
    "test:full": "npm run test:unit && npm run test:eslint && npm run test:coveralls",
    "test:eslint": "webpack --config webpack.config.dev.js",
    "test:coverage": "./node_modules/.bin/babel-istanbul cover --include-all-sources --root ./src ./node_modules/.bin/_mocha ./src/**/__tests__/*.js",
    "test:watch": "npm test -- --watch",
    "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "node server.js",
    "build": "npm run clean && npm run build:webpack",
    "translate": "bash fetch-translation.sh",
    "nativefier": "./node_modules/.bin/nativefier"
  },
  "keywords": [
    "binary.com",
    "webpack",
    "react",
    "redux",
    "websockets"
  ],
  "author": "Boris @ Binary",
  "license": "MIT",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-function-bind",
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "binary-live-api": "*",
    "echarts": "^3.0.2",
    "history": "^2.0.0",
    "immutable": "^3.7.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-draggable": "^1.1.1",
    "react-intl": "^2.0.0-beta-1",
    "react-pure-render": "^1.0.2",
    "react-redux": "4.3.0",
    "react-router": "^2.0.0-rc5",
    "react-sparklines": "*",
    "react-youtube": "^5.0.0",
    "redux": "^3.2.0",
    "reselect": "^2.0.3",
    "redux-storage": "^2.0.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-intl": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^2.11.6",
    "enzyme": "^1.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "express": "^4.13.4",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "nativefier": "^6.8.0",
    "react-addons-perf": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "redux-mock-store": "0.0.6",
    "webpack": "^2.0.6-beta",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.2"
  }
}
